#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/mirko/OneDrive/Desktop/gICM/node_modules/.pnpm/openai@6.9.1_ws@8.18.3_buff_964f9843b846d1c692b975c9e198eac7/node_modules/openai/bin/node_modules:/proc/cygdrive/c/Users/mirko/OneDrive/Desktop/gICM/node_modules/.pnpm/openai@6.9.1_ws@8.18.3_buff_964f9843b846d1c692b975c9e198eac7/node_modules/openai/node_modules:/proc/cygdrive/c/Users/mirko/OneDrive/Desktop/gICM/node_modules/.pnpm/openai@6.9.1_ws@8.18.3_buff_964f9843b846d1c692b975c9e198eac7/node_modules:/proc/cygdrive/c/Users/mirko/OneDrive/Desktop/gICM/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/mirko/OneDrive/Desktop/gICM/node_modules/.pnpm/openai@6.9.1_ws@8.18.3_buff_964f9843b846d1c692b975c9e198eac7/node_modules/openai/bin/node_modules:/proc/cygdrive/c/Users/mirko/OneDrive/Desktop/gICM/node_modules/.pnpm/openai@6.9.1_ws@8.18.3_buff_964f9843b846d1c692b975c9e198eac7/node_modules/openai/node_modules:/proc/cygdrive/c/Users/mirko/OneDrive/Desktop/gICM/node_modules/.pnpm/openai@6.9.1_ws@8.18.3_buff_964f9843b846d1c692b975c9e198eac7/node_modules:/proc/cygdrive/c/Users/mirko/OneDrive/Desktop/gICM/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../openai/bin/cli" "$@"
else
  exec node  "$basedir/../openai/bin/cli" "$@"
fi
