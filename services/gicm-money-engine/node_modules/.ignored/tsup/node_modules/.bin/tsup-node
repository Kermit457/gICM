#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/mirko/OneDrive/Desktop/gICM/node_modules/.pnpm/tsup@8.5.1_jiti@1.21.7_post_3c7aaa37caec935d842aac3c5e77a49d/node_modules/tsup/dist/node_modules:/proc/cygdrive/c/Users/mirko/OneDrive/Desktop/gICM/node_modules/.pnpm/tsup@8.5.1_jiti@1.21.7_post_3c7aaa37caec935d842aac3c5e77a49d/node_modules/tsup/node_modules:/proc/cygdrive/c/Users/mirko/OneDrive/Desktop/gICM/node_modules/.pnpm/tsup@8.5.1_jiti@1.21.7_post_3c7aaa37caec935d842aac3c5e77a49d/node_modules:/proc/cygdrive/c/Users/mirko/OneDrive/Desktop/gICM/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/mirko/OneDrive/Desktop/gICM/node_modules/.pnpm/tsup@8.5.1_jiti@1.21.7_post_3c7aaa37caec935d842aac3c5e77a49d/node_modules/tsup/dist/node_modules:/proc/cygdrive/c/Users/mirko/OneDrive/Desktop/gICM/node_modules/.pnpm/tsup@8.5.1_jiti@1.21.7_post_3c7aaa37caec935d842aac3c5e77a49d/node_modules/tsup/node_modules:/proc/cygdrive/c/Users/mirko/OneDrive/Desktop/gICM/node_modules/.pnpm/tsup@8.5.1_jiti@1.21.7_post_3c7aaa37caec935d842aac3c5e77a49d/node_modules:/proc/cygdrive/c/Users/mirko/OneDrive/Desktop/gICM/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli-node.js" "$@"
else
  exec node  "$basedir/../../dist/cli-node.js" "$@"
fi
