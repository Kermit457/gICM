{
  "name": "@gicm/skill-ai-toolkit",
  "version": "1.0.0",
  "description": "AI/LLM integration utilities for OPUS 67 - RAG pipelines, embeddings, streaming, and agent patterns",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./rag": {
      "types": "./dist/rag.d.ts",
      "import": "./dist/rag.js"
    },
    "./embeddings": {
      "types": "./dist/embeddings.d.ts",
      "import": "./dist/embeddings.js"
    },
    "./streaming": {
      "types": "./dist/streaming.d.ts",
      "import": "./dist/streaming.js"
    },
    "./agents": {
      "types": "./dist/agents.d.ts",
      "import": "./dist/agents.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "llm",
    "rag",
    "embeddings",
    "claude",
    "anthropic",
    "langchain",
    "opus67"
  ],
  "author": "Mirko Basil DÃ¶lger",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gicm-platform/gICM.git",
    "directory": "packages/skill-ai-toolkit"
  },
  "dependencies": {
    "zod": "^3.23.0"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": ">=0.24.0",
    "ai": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "@anthropic-ai/sdk": {
      "optional": true
    },
    "ai": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  }
}
