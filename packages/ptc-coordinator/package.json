{
  "name": "@gicm/ptc-coordinator",
  "version": "1.0.1",
  "description": "Programmatic Tool Calling (PTC) Coordinator - Orchestrate gICM agents via generated code pipelines",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./templates": {
      "types": "./dist/templates/index.d.ts",
      "import": "./dist/templates/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/templates/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts src/templates/index.ts --format esm --dts --watch"
  },
  "dependencies": {
    "@gicm/agent-core": "^1.0.0",
    "eventemitter3": "^5.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "tsup": "^8.0.0",
    "@types/node": "^20.0.0"
  },
  "files": ["dist", "README.md"],
  "keywords": [
    "ptc",
    "programmatic-tool-calling",
    "agent-orchestration",
    "pipeline",
    "gicm"
  ],
  "author": "Mirko Basil DÃ¶lger <mirko@gicm.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gicm-dev/gicm.git",
    "directory": "packages/ptc-coordinator"
  },
  "homepage": "https://github.com/gicm-dev/gicm/tree/main/packages/ptc-coordinator#readme",
  "bugs": { "url": "https://github.com/gicm-dev/gicm/issues" },
  "publishConfig": { "access": "public" }
}
